<p id="notice"><%= notice %></p>

<h1>Listing Notes</h1>

<table class= 'table table-hover'>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Time</th>
      <th>Due date</th>
      <th>Created at</th>
      <th>Updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= render @notes %>
  </tbody>
</table>

<br>

<%= link_to 'New Note', new_note_path %>
